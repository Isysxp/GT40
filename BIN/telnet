set cpu 11/05
set cpu 28k
set dli enable
set dli line=2
set dlo 8b
set dli 8b
set dlo0 dataset
set dz disable
set vt crt=vr14
set vt scale=2
set vt alias=on
set vt enable
set dli address=17775610
att dli line=0,connect=%1:%2
set rom0 address=17766000
at rom0 bootvt.img
load vt07.bin
;boot rom0
;run 1000
